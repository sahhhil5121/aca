#include <iostream>
using namespace std;

class Node {
  public:
    int data;
    Node *next;

    Node(int new_data) {
        data = new_data;
        next = nullptr;
    }
};

Node *reverseList(Node *head) {

    Node *curr = head, *prev = nullptr, *next;

    while (curr != nullptr) {
        next = curr->next;
        curr->next = prev;

        prev = curr;
        curr = next;
    }

    return prev;
}

void printList(Node *node) {
    while (node != nullptr) {
        cout << node->data;
        if (node->next)
            cout << " -> ";
        node = node->next;
    }
}

int main() {
    int n, x;
    cout << "Enter number of nodes: ";
    cin >> n;

    Node* head = NULL;
    Node* temp = NULL;

    cout << "Enter values:\n";
    for(int i=0; i<n; i++){
        cin >> x;

        if(head == NULL){
            head = new Node(x);
            temp = head;
        }
        else{
            temp->next = new Node(x);
            temp = temp->next;
        }
    }

    head = reverseList(head);

    cout << "Reversed List: ";
    while(head){
        cout << head->data << " ";
        head = head->next;
    }
}
