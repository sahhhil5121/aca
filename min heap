#include<iostream>
#include<queue>
#include<vector>
using namespace std;
void insert(int i,vector<int>&arr,vector<int>&ans){
    ans.push_back(arr[i]);
    int idx=ans.size()-1;
    while(idx>0&&ans[(idx-1)/2]>ans[idx]){
        swap(ans[(idx-1)/2],ans[idx]);
        idx=(idx-1)/2;
    }
}
int main(){
    vector<int>arr{1,5,6,8,4,9};
    int n=arr.size();
    vector<int>ans;
    for(int i=0;i<n;i++){
        insert(i,arr,ans);
        cout<<arr[i]<<"inserted element is:"<<" ";
        for(int j=0;j<ans.size();j++){
            cout<<ans[j]<<" ";
        }
        cout<<endl;
    }
}
